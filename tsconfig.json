{
    "compilerOptions": {
        "target": "es6",
        "noImplicitAny": false,
        "outDir": "built",
        "rootDir": ".",
        "sourceMap": false,
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
            "@app/*": ["src/*"],
            "@http/*": ["src/http/*"],
            "@socket/*": ["src/socket/*"],
            "@endpoints/*": ["src/endpoints/*"],      
        }
    },
    "exclude": [
        "node_modules"
    ]
}